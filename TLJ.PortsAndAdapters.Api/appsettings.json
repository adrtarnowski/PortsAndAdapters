{
  "App": {
    "Name": "TLJ.ApiTemplate",
    "Version": "0.0.1"
  },
  "Logging": {
    "ApplicationInsights": {
      "InstrumentationKey": "<provide-instrumentation-key>"
    },
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "Swagger": {
    "ApiName": "Tech Leader Journey - Ports and Adapters .NET Template",
    "ApiVersion": "v1",
    "ApiDescription": "API documentation"
  },
  "WebApi": {
    "CorsAllowedOrigins": [ "http://localhost:3000" ]
  },
  "HttpClient": {
    "Retries": 3
  },
  "AppConfiguration": {
    "Endpoint": "",
    "ConnectionString": ""
  },
  "Database": {
      "ConnectionString": "Server=localhost;Database=TLJ;Trusted_Connection=True;Integrated Security=True;TrustServerCertificate=True;MultipleActiveResultSets=true"
  },
  "MessageBus": {
    "ConnectionString": "<provide-connection-string>",
    "QueueName": "<provide-queue-name>",
    "EventTopicName": "<provide-topic-name>",
    "EventSubscriptionName": "<provide-subscription-name>",
    "AutoComplete": false,
    "MaxConcurrentCalls": 16,
    "CommandMaxConcurrentCalls": 16,
    "MaxAutoRenewMinutesDuration": 5,
    "MessageWaitTimeoutInSeconds": 1
  },
  "AzureAD": {
    "TLJ": {
      "Instance": "https://login.microsoftonline.com/",
      "TenantId": "<tenant id>",
      "ClientId": "<client id>",
      "ClientSecret": "<secret>"
    }
  }
}